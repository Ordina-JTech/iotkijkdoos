<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/IVTelevision"
        android:layout_width="198dp"
        android:layout_height="63dp"
        android:transitionName="ViewBoxComponent"
        app:srcCompat="@drawable/tv_meubel_illus4_bewerkt_licht_minder_nerf"
        android:layout_marginTop="88dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent" />

    <ImageView
        android:layout_width="71dp"
        android:layout_height="85dp"
        app:srcCompat="@drawable/lamp_foto2"
        android:id="@+id/imageView"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/IVTelevision"
        android:layout_marginEnd="8dp" />

    <ImageView
        android:layout_width="71dp"
        android:layout_height="85dp"
        app:srcCompat="@drawable/lamp_foto2"
        android:id="@+id/imageView2"
        app:layout_constraintLeft_toRightOf="@+id/IVTelevision"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:layout_width="214dp"
        android:layout_height="242dp"
        app:srcCompat="@drawable/fauteuil_foto_auteursrechten2_bewerkt"
        android:id="@+id/imageView5"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="48dp"
        app:layout_constraintTop_toBottomOf="@+id/IVTelevision" />

    <ImageView
        android:layout_width="170dp"
        android:layout_height="197dp"
        app:srcCompat="@drawable/instrument_1"
        android:id="@+id/imageView6"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent" />

    <ImageView
        android:layout_width="77dp"
        android:layout_height="129dp"
        app:srcCompat="@drawable/popcorn_auteursrecht1"
        android:id="@+id/imageView7"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/IVTelevision" />

</android.support.constraint.ConstraintLayout>